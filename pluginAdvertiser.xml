<application>
  <component name="PluginAdvertiserExtensions">
    <option name="localState">
      <map>
        <entry key="*.env">
          <value>
            <plugin>
              <option name="myPluginId" value="ru.adelf.idea.dotenv" />
              <option name="myPluginName" value=".env files support" />
            </plugin>
          </value>
        </entry>
        <entry key=".env">
          <value>
            <plugin>
              <option name="myPluginId" value="ru.adelf.idea.dotenv" />
              <option name="myPluginName" value=".env files support" />
            </plugin>
          </value>
        </entry>
        <entry key=".env.*">
          <value>
            <plugin>
              <option name="myPluginId" value="ru.adelf.idea.dotenv" />
              <option name="myPluginName" value=".env files support" />
            </plugin>
          </value>
        </entry>
      </map>
    </option>
  </component>
</application>